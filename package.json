{
  "name": "psnext.info",
  "version": "0.1.0",
  "description": "server",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src -r dotenv/config src/server.js",
    "test": "NODE_ENV=test LOG_LEVEL=error nyc --reporter=html --reporter=text mocha --exit -r dotenv/config --require should --reporter spec --timeout 10000",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "cd ui && npm run build && cp -r build ../"
  },
  "mocha": {
    "file": [
      "./test/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UnknownInc/psi.git"
  },
  "author": "",
