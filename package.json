  "bugs": {
    "url": "https://github.com/UnknownInc/psi/issues"
  },
  "homepage": "https://github.com/UnknownInc/psi#readme",
  "dependencies": {
    "cookie-parser": "1.4.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.7.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.5.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uninstall": "0.0.0",
    "uuid": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "coveralls": "3.1.0",
